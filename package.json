{
  "name": "@notkeira/ttl-cache",
  "version": "0.1.0",
  "description": "Lightweight LRU cache with TTL support and optional discord.js Collection compatibility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "pnpm build",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "cache",
    "lru",
    "ttl",
    "discord.js",
    "collection",
    "typescript"
  ],
  "author": "Keira Hopkins <node-pkgs@accounts.keira.boo> (https://keirahopkins.co.uk)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "discord.js": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "discord.js": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NotKeira/ttl-cache"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.19.0+sha256.1c5f5ee59267e4e2e1bfd335f432a18dc48f9a9944094d5d2649757e725ad376",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2"
  }
}
